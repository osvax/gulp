# editorconfig.org
root = true

[*]
end_of_line = lf
#indent_style = space
indent_size = 2
charset = utf-8
#trim_trailing_whitespace = true
#insert_final_newline = true

[*.md]
trim_trailing_whitespace = false
# Ignore paths
[/.idea/**]
charset = unset
end_of_line = unset
insert_final_newline = unset
trim_trailing_whitespace = unset
indent_style = unset
indent_size = unset

